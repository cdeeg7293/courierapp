<div class="row-fluid">
  <div class="span9" id="content">
    

    <div class="row-fluid">
      <!-- block -->
      <div class="block">
        <div class="navbar navbar-inner block-header">
          <div class="muted pull-left">Listing clients</div>
          <%= link_to 'New Client', new_client_path, class: "pull-right btn btn-primary" %>
        </div>
        <div class="block-content collapse in">
          <div class="span12">
            <%= render partial: "clients/clients_list", locals: { clients: @clients, return_url: @return_url } %>
          </div>
        </div>
        <%= will_paginate @clients, renderer: BootstrapPagination::Rails %>
      </div>
      <!-- /block -->
    </div>
  </div>
  <div class="span3" id="sidebar">
    Dis iz da sidebar
  </div>
</div>

